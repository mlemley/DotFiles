import __package__.interfaces.__model_interface__

public class __name__ extends Proxy
{

    //---------------------------------------------------
    //
    //  Class Constants
    //
    //---------------------------------------------------

    public static const NAME:String = '__name__';

    //---------------------------------------------------
    //
    //  Constructor
    //
    //---------------------------------------------------

    public function (model:__model_interface__):void
    {
        super(NAME);
        setData(model);
    }

    //---------------------------------------------------
    //
